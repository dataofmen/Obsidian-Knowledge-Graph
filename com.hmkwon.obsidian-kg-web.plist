<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.hmkwon.obsidian-kg-web</string>

    <key>ProgramArguments</key>
    <array>
        <string>/opt/homebrew/bin/uv</string>
        <string>run</string>
        <string>kg</string>
        <string>web</string>
        <string>--port</string>
        <string>8765</string>
    </array>

    <key>WorkingDirectory</key>
    <string>/Users/hmkwon/Project/027_KnowlegeGraph</string>

    <key>EnvironmentVariables</key>
    <dict>
        <key>PATH</key>
        <string>/opt/homebrew/bin:/usr/local/bin:/usr/bin:/bin</string>
    </dict>

    <key>RunAtLoad</key>
    <true/>

    <key>KeepAlive</key>
    <dict>
        <key>SuccessfulExit</key>
        <false/>
    </dict>

    <key>StandardOutPath</key>
    <string>/Users/hmkwon/Project/027_KnowlegeGraph/logs/kg-web.out.log</string>
    <key>StandardErrorPath</key>
    <string>/Users/hmkwon/Project/027_KnowlegeGraph/logs/kg-web.err.log</string>

    <!-- Neo4j가 먼저 시작될 시간을 줌 -->
    <key>ThrottleInterval</key>
    <integer>30</integer>
</dict>
</plist>
