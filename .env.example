# Primary LLM: OpenRouter (OpenAI-compatible)
OPENROUTER_API_KEY=your_key_here
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1
OPENROUTER_MODEL=openai/gpt-4o-mini
OPENROUTER_SMALL_MODEL=openai/gpt-4o-mini

# Secondary LLM: Ollama Cloud (OpenAI-compatible)
OLLAMA_CLOUD_API_KEY=your_key_here
OLLAMA_CLOUD_BASE_URL=https://api.ollama.com/v1
OLLAMA_CLOUD_MODEL=llama3.1:70b
OLLAMA_CLOUD_SMALL_MODEL=llama3.1:8b

# Neo4j
NEO4J_URI=bolt://localhost:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=your_password_here

# Obsidian Vault
OBSIDIAN_VAULT_PATH=/path/to/your/vault
